#include <stdio.h>
int main()
{
    int i,j,mul,n;
    printf("enter n value: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            mul=i*j;
            printf("%d\t",mul);   
        }
        printf("\n");
    }

    return 0;
}

Input : 
Enter n value:5
Output: 
1       2       3       4       5
2       4       6       8       10
3       6       9       12      15
4       8       12      16      20
5       10      15      20      25
