#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char s[1000];
    gets(s);
    int x[10]={0};
    int i;
    for(i=0;s[i]!=0;i++)
    {
        if(s[i]>='0' && s[i]<='9')
        {
            int val=s[i]-'0';
            x[val]=x[val]+1;
        }
    }
    for(i=0;i<10;i++)
    {
        printf("%d ",x[i]);
    }
    return 0;
}

sample input1:
a11472o5t6
sample output1:
0 2 1 0 1 1 1 1 0 0 
